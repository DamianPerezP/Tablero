@using System.Data;
@{
    DataSet currentDataSet = (DataSet)ViewBag.ElDataSet;
}
   <div class="container">
    <h2>Basic Table</h2>
       <table class="table">
           <thead>
               <tr>
                   @for (int i = 0; i < currentDataSet.Tables[0].Rows.Count; i++)
                   {
                       <th>@currentDataSet.Tables[0].Rows[i][0]</th>
                   }
               </tr>
           </thead>
           <tbody>
               @for (int i = 0; i < currentDataSet.Tables[0].Rows.Count; i++)
               {
                   <tr>
                           <td>@currentDataSet.Tables[0].Rows[i][i++]</td>
                   </tr>
               }
           </tbody>
       </table>
</div>
