@using System.Data;
@{
    DataSet currentDataSet = (DataSet)ViewBag.Nombres;
}    


<div id="wrapper">
                <div id="page-wrapper">
            <div class="container-fluid">
                <!-- /.row -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i> Panel Chart</h3>
                            </div>
                            <div class="panel-body">
                                <div id="donut-example"></div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->

            </div>
        </div>
    </div>

    <!-- jQuery -->
    <script src="~/js/jquery.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/js.js"></script>
    <!-- Morris Charts JavaScript -->
    <script src="~/js/plugins/morris/raphael.min.js"></script>
    <script src="~/js/plugins/morris/morris.min.js"></script>

    <!--
    <script src="js/plugins/morris/morris-data.js"></script>
    -->
    <script>
        $(function() {
            Morris.Donut({
                element: 'donut-example',
                data: [
                     @for (var i = 0; i < currentDataSet.Tables[0].Rows.Count; i++)
                     {
                     <text>
                            {   valor: '@currentDataSet.Tables[0].Rows[i][0]',
                                valor2: '@currentDataSet.Tables[0].Rows[i][1]',
                                valor3: @currentDataSet.Tables[0].Rows[i][2]
                                }@((i < currentDataSet.Tables[0].Rows.Count - 1) ? "," : "")
                        </text>
                         }
                ],

                label: "Download Sales", value: 12,
                label: "In-Store Sales", value: 30,
                label: "Mail-Order Sales", value: 20,
            
            });
        });

    </script>

    